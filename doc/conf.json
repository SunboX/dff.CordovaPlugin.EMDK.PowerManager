{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["www"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "templates/default",     // same as -t templates/default
        "encoding": "utf8",                  // same as -e utf8
        "destination": "./doc/jsdoc",        // same as -d ./out/
        "recurse": true,                     // same as -r
        "tutorials": "./doc/jsdoc/tutorials" // same as -u path/to/tutorials
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    }
}

